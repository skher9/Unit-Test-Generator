# Database
DATABASE_URL="postgresql://USER:PASSWORD@localhost:5432/unit_test_generator?schema=public"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server (optional)
PORT=3000
NODE_ENV=development
CORS_ORIGIN=http://localhost:5173

# AI provider (only deepseek supported)
AI_PROVIDER=deepseek

# DeepSeek (required for POST /generations when AI_PROVIDER=deepseek)
DEEPSEEK_API_KEY=
DEEPSEEK_MODEL=deepseek-coder
# Optional: DEEPSEEK_MAX_TOKENS=4000, DEEPSEEK_TEMPERATURE=0.2
